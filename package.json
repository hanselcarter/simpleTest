{
  "name": "calendar",
  "version": "1.0.0",
  "description": "having fun with react",
  "main": "index.js",
  "repository": "git@github.com:hanselcarter/calendar.git",
  "scripts": {
    "start": "webpack-dev-server --config webpack/dev.config.js",
    "start:prod": "cross-env NODE_ENV=production npm run start",
    "start:dev": "cross-env NODE_ENV=development npm run start",
    "test": "jest --silent",
    "test:coverage": "jest --coverage",
    "build:prod": "npm run test && npm run build-all:prod",
    "build:dev": "npm run test && npm run build-all:dev",
    "build-all": "webpack --config webpack/prod.config.js",
    "build-all:prod": "cross-env NODE_ENV=production npm run build-all",
    "lint": "eslint --ext .jsx --ext .js ./src",
    "prettier-all": "prettier --config .prettierrc --write \"src/**/*.js\"",
    "release": "standard-version"
  },
  "author": "Hansel Carter",
  "license": "MIT",
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@material-ui/core": "^4.9.2",
    "@material-ui/icons": "^3.0.2",
    "@material-ui/lab": "^4.0.0-alpha.39",
    "@sendgrid/mail": "^6.5.2",
    "@sentry/browser": "^5.12.1",
    "@typeform/embed": "^0.15.3",
    "axios": "^0.19.2",
    "clsx": "^1.1.0",
    "email-validator": "^2.0.4",
    "firebase": "^7.8.1",
    "history": "^4.10.1",
    "html-to-text": "^5.1.1",
    "json2csv": "^4.5.4",
    "lodash": "^4.17.15",
    "micro": "^9.3.4",
    "moment": "^2.24.0",
    "moment-duration-format": "^2.3.2",
    "notistack": "^0.9.7",
    "prop-types": "^15.7.2",
    "query-string": "^6.10.1",
    "react": "^16.12.0",
    "react-csv": "^1.1.2",
    "react-datepicker": "^2.14.1",
    "react-dom": "^16.12.0",
    "react-google-button": "^0.5.3",
    "react-player": "^1.15.2",
    "react-quill": "^1.3.3",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-swipeable-views": "^0.13.9",
    "redux": "^4.0.5",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.2.0",
    "slack": "^11.0.1",
    "style-loader": "^0.23.1",
    "throttle-debounce": "^2.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@types/history": "^4.7.5",
    "@types/jest": "^24.9.1",
    "@types/react-router-dom": "^4.3.5",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-polyfill": "^6.26.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^5.2.1",
    "css-loader": "^2.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.3.0",
    "file-loader": "^5.0.2",
    "firebase-admin": "^8.9.2",
    "firebase-functions": "^3.3.0",
    "firebase-tools": "^7.16.1",
    "generate-json-webpack-plugin": "^0.3.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "jest-when": "^2.7.0",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "redux-devtools-extension": "^2.13.8",
    "standard-version": "^7.1.0",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>tests/setup/setupEnzyme.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>tests/setup/"
    ],
    "setupFiles": [
      "<rootDir>node_modules/regenerator-runtime/runtime"
    ],
    "collectCoverageFrom": [
      "!**/node_modules/**",
      "!<rootDir>/src/*.{js,jsx}",
      "!<rootDir>/src/components/*.{js,jsx}",
      "<rootDir>/src/**/*.{js,jsx}"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/fileMockTransformer.js"
    },
    "moduleNameMapper": {
      "Components/(.*)$": "<rootDir>/src/components/$1",
      "Actions/(.*)$": "<rootDir>/src/actions/$1",
      "Assets/(.*)$": "<rootDir>/assets/$1",
      "calendarReduxHooks": "<rootDir>/src/calendarReduxHooks",
      "\\.(css)$": "<rootDir>/tests/stylesMock.js"
    }
  },
  "husky": {
    "hooks": {
      "pre-push": "npm t"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint",
      "prettier --write"
    ]
  },
  "engines": {
    "node": "8"
  }
}
